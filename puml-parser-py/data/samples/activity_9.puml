@startuml
skinparam activity {
    BackgroundColor<<API>> #E3F6FF
    BackgroundColor<<Manager>> #E8F6E8
    BackgroundColor<<Callback>> #FFF6E3
}
skinparam shadowing<<no_shadow>> false
skinparam shadowing<<with_shadow>> true

